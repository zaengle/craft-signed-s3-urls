{
    "name": "zaengle/craft-awss3signedurls",
    "description": "Generate signed URLs for CraftCMS AWS S3 Craft Asset Volumes, including the ability to limit access.",
    "type": "craft-plugin",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "yiisoft/yii2-composer": true,
            "craftcms/plugin-installer": true
        },
        "sort-packages": true,
        "optimize-autoloader": true

    },
    "autoload": {
        "psr-4": {
            "zaengle\\awss3signedurls\\": "src/"
        }
    },
    "support": {
        "docs": "https://github.com/zaengle/craft-awss3signedurls#README.md",
        "issues": "https://github.com/zaengle/craft-awss3signedurls/issues",
        "source": "https://github.com/zaengle/craft-awss3signedurls/tree/main"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Zaengle Corp",
            "homepage": "https://zaengle.com/"
        }
    ],
    "require": {
        "php": "^8.2 | ^8.3 | ^8.4"
    },
    "require-dev": {
        "craftcms/aws-s3": "^2.2",
        "craftcms/cms": "^5.1",
        "craftcms/ecs": "dev-main",
        "craftcms/phpstan": "dev-main",
        "craftcms/rector": "dev-main",
        "roave/security-advisories": "dev-latest"
    },
    "scripts": {
        "check-cs": "ecs check --ansi",
        "coverage": "pest --coverage",
        "fix-cs": "ecs check --ansi --fix --config ./ecs.php",
        "phpstan": "vendor/bin/phpstan analyse --memory-limit=1G",
        "test": "pest"
    },
    "extra": {
        "name": "AWS S3 Signed URLs",
        "handle": "awss3signedurls",
        "developer": "Zaengle Corp",
        "developerUrl": "https://zaengle.com",
        "documentationUrl": "https://github.com/zaengle/craft-awss3signedurls/blob/master/README.md",
        "changelogUrl": "https://raw.githubusercontent.com/zaengle/craft-awss3signedurls/master/CHANGELOG.md",
        "components": {
            "challenges": "zaengle\\awss3signedurls\\services\\Challenges",
            "fs": "zaengle\\awss3signedurls\\services\\Fs",
            "signer": "zaengle\\awss3signedurls\\services\\Signer"
        },
        "class": "zaengle\\awss3signedurls\\AwsS3SignedUrls"
    }
}
